  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="lightdb.struct.lib" Version="1.0.1" />
    <PackageReference Include="System.Security.Cryptography.Algorithms" Version="4.3.1" />
  </ItemGroup>
</Project>
